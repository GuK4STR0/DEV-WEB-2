<header div="">
    <nav class="navbar navbar-expand-lg bg-primary">
        <div class="container-xl ">
            <a class="navbar-brand text-white" href="#">Navbar</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNavDropdown"
                aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNavDropdown">
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link active text-white" aria-current="page" href="#">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-white" href="/consulta">Consulta</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
</header>
<main class="container-xl p-3 d-flex flex-column gap-2">
    <div>
        <h5>Consulta - Agendamento de Potenciais Clientes</h5>
        <small>Sistema utilizado para agendamentos de serviços</small>
    </div>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Nome</th>
                <th scope="col">Telefone</th>
                <th scope="col">Origem</th>
                <th scope="col">Data de contato</th>
                <th scope="col">Observação</th>
            </tr>
        </thead>
        <tbody>
        {{#each post}}
            {{#dataValues}}    
                    <tr>
                        <td>{{nome}}</td>
                        <td>{{telefone}}</td>
                        <td>{{origem}}</td>
                        <td>{{data}}</td>
                        <td>{{observacao}}</td>
                    </tr>
            {{/dataValues}}
        {{/each}}
        </tbody>
    </table>
</main>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"></script>